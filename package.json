{
  "name": "stata-outline",
  "displayName": "Stata Outline",
  "description": "Adds hierarchical Outline support for Stata .do files using comment headings like '* %%', '* %%%', etc.",
  "version": "0.0.5",
  "publisher": "ZihaoVistonWang",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onLanguage:stata"
  ],
  "main": "./extension.js",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "stata",
        "aliases": ["Stata", "stata"],
        "extensions": [".do"]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZihaoVistonWang/stata-outline"
  }
}
